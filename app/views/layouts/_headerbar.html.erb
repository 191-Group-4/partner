<div style="float: left; width: 39%">
  <a href="/"><img width=100% src="/assets/PartnerUpLogo.png"></a>
</div>
<div style="float: left; width: 11%">
	
</div>
<div style="float: right; width: 50%; text-align:right;">

<%if user_signed_in? %>
<%= link_to 'Edit account', edit_user_registration_path %> <br>
<%= link_to 'Sign out', destroy_user_session_path, :method => :delete %>
<% else %>
<%if @ucinetid.blank?%>
<%= link_to 'Login', "https://login.uci.edu/" %><br>
<%else%>
  <%= form_for(resource, :url => user_session_path, :html => { :role => 'form'}) do |f| %>
    <%= devise_error_messages! %>
    <%= f.hidden_field :campus_id, :value => @campus_id %> 
    <%= f.hidden_field :ucinetid, :value => @ucinetid %> 
    <%= f.submit 'Sign in' %>
  <% end %>



<% end %><% end %>
</div>
  <br style="clear: both;" />
</div>