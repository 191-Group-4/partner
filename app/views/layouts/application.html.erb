<html>
<%= favicon_link_tag 'favicon.ico' %>
  <head>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title><%= content_for?(:title) ? yield(:title) : "Partner" %></title>
    <meta name="description" content="<%= content_for?(:description) ? yield(:description) : "Partner" %>">
    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track' => true %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
    <%= csrf_meta_tags %>
  </head>
  <body>
    <header>
<%=@auth_fail%> <%=@error_code%><%=@ucinetid%> <%=@campus_id%> <%=@auth_host%> <%=@age_in_seconds%> <%=@uci_affiliations%>
      <%= render 'layouts/headerbar' %>   
      <%= render 'layouts/navigation' %>
    </header>
    <main role="main">
       <%= render 'layouts/messages' %>
       <%= yield %>
    </main>
  </body>
  <footer>      
    <%= render 'layouts/footer' %>     
  </footer>
</html>
